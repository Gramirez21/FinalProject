@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Homepage</title>
    <link rel="stylesheet" href="path/to/homepage.css">
</head>
<body>

    <!-- Header -->
    <div class="header">
        <h1>Luffy Security.Inc</h1>
    </div>

    <!-- Navigation Menu -->
    <div class="nav-menu">
        <ul>
            <li><a href="#">Home</a></li>
            <li><a href="#">Employees</a></li>
            
    </div>

    <!-- Hero Section -->
    <div class="hero">
        <h2>Welcome to Our Administration Website</h2>
        <p>Employee Log</p>
    </div>

    <!-- Featured Content Section -->
    <div class="featured-content">
        <h3>Featured Content</h3>
        <p>Select employees for more options</p>
    </div>

    <!-- Image -->
    <style>
        body{
            background-image: url("/Images/cat.jpg");
            background-position:left;
            background-size:picture;
        }

    </style>

    <!-- Footer -->
    <div class="footer">
        <p>&copy; 2024 Luffy Secrity.inc. All rights reserved.</p>
    </div>

</body>
</html>

