@model IEnumerable<Employee>

<h2>Employees</h2>

<div style="display: block">
    <a href="/Employee/InsertEmployee/">Add Employee</a>
</div>

<table class="table">
    <tr>
        <th>Employee ID</th>
        <th>Name</th>
        <th>Last</th>
        <th>Email</th>
        <th>Department</th>
        <th>Salary</th>
        <th>Date Hired</th>
    </tr>

    @foreach (var employee in @Model)
    {
        <tr>
            <td><a href="/employee/Viewemployee/@employee.EmployeeID">@employee.EmployeeID</a></td>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>@employee.Email</td>
            <td>@employee.Department</td>
            <td>@employee.Salary</td>
            <td>@employee.DateHired</td>
        </tr>
    }

</table>